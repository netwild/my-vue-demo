{
  "eslintIntegration": true,
  "requireConfig": false,
  "printWidth": 100,
  "semi": false,
  "singleQuote": true,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "ignore",
  "ignorePath": ".prettierignore",
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": false
}
